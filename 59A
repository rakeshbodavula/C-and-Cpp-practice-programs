#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main(){
    int count=0;
    char str[100];
    scanf("%s",str);
    for(int i=0;i<strlen(str);i++){
        int ascii=(int)str[i];
        if(ascii<97) count++;
        else count--;
    }
if(count>0)
    for(int i=0;i<strlen(str);i++) printf("%c",toupper(str[i]));
else for(int i=0;i<strlen(str);i++) printf("%c",tolower(str[i]));
printf("\n");
return 0;
}
